generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Referral {
  id            Int      @id @default(autoincrement())
  referrerName  String
  referrerEmail String
  referredName  String
  referredEmail String
  createdAt     DateTime @default(now())
  pending       Boolean  @default(true)
}
